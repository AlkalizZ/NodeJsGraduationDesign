<article class="archive-article archive-type-post" data-id="<%- key.title %>" date-time="<%= key.date %>" date-description="<%= key.searchDescription %>">
    <div class="archive-article-inner">
        <header class="archive-article-header">
            <div class="article-meta">
                <a href="/docs/<%- key.title %>.md" class="archive-article-date" title="点击导出文章" download>
                    <time datetime="<%= key.date %>" itemprop="datePublished"><%= key.date %></time>
                </a>
            </div>
            <h1 itemprop="name">
                <a class="archive-article-title" href="<%= key.postUrl %>"><%= key.title %></a>
            </h1>
            <div class="article-info info-on-archive">
                <div class="article-tag tagcloud">
                    <ul class="article-tag-list">
                        <% for(i of key.tags){ %>
                        <li class="article-tag-list-item"><a class="article-tag-list-link" href="/tags/<%= i %>/"><%= i %></a></li>
                        <% } %>
                    </ul>
                </div>
            </div>
            <div class="clearfix"></div>
        </header>
    </div>
</article>